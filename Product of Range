#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a,fact=1;
    scanf("%d",&a);
    if(a>=0){
    for(int i=1;i<=a;i++){
        fact*=i;
    }
    printf("Product of value is %d",fact);
    }
    else{
        printf("Invalid Input");
    }  
    return 0;
}
