#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
     int a;
    scanf("%d",&a);
    int b=a*a;
    int c=b%10;
    if(a<=9&&a>=0){
    if(a==c){
        printf("Automorphic Number");
    }
    else{
        printf("Not Automorphic Number");
    }
}
    else{
        printf("Invalid Input");
    }
    return 0;
}
