#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int t,v;
    scanf("%d %d",&t,&v);
    printf("%.2f", 35.74 + 0.6215*t + (0.4275*t - 35.75) * pow(v,0.16));
                                                              
    return 0;
}
