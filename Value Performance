#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int a;
    float f;
    scanf("%d",&a);
    scanf("%f",&f);
    printf("%d\n",a);
    printf("%.2f",f);
    return 0;
}
