#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,k;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    scanf("%d",&k);
   int count=0;
    for(int i=0;i<n;i++){
        if(k==arr[i]){
            count++;
        }
    }
    if(count>0){
        printf("%d is presented in an array.",k);
    }
    else{
        printf("%d is not presented in an array.",k);
    }
    return 0;
}
