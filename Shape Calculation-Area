#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a,l,b,r,ba,he;
    scanf("%d %d %d %d %d %d",&a,&l,&b,&r,&ba,&he);
    printf("Area of Square=%d\n",a*a);
    printf("Area of Rectangle=%d\n",l*b);
    printf("Area of Circle=%.2f\n",floor((3.14159*r*r)*100)/100);
    printf("Area of Triangle=%.0f",0.5*(ba*he));
    return 0;
}
