#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a,b;
    scanf("%d %d",&a,&b);
    printf("Addition=%d\n",a+b);
    printf("Subtraction=%d\n",a-b);
    printf("Multiplication=%d\n",a*b);
    printf("Division=%d\n",a/b);
    printf("Modulo=%d",a%b);
    return 0;
}
