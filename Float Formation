#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    float a;
    scanf("%f",&a);
    printf("%.6f\n",a);
    printf("%.4f\n",a);
    printf("%.2f\n",a);
    printf("%.0f",a);
    
    return 0;
}
